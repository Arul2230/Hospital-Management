<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 5.3//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">
 <hibernate-mapping>
  <class name="com.ideas2it.hospitalmanagement.visit.model.Visit" table="visit">
    <id name="id" column="id">
    <generator class="increment"></generator>
    </id>
    <property name="admitDate" column="ADMIT_DATE" type="date"/>
    <property name="dischargeDate" column="DISCHARGE_DATE" type="date"/>

    <!--<many-to-one name="physician" class="com.ideas2it.hospitalmanagement.physician.model.Physician" column="PHYSICIAN" cascade="all" not-null="true" />
    <many-to-one name="patient" class="com.ideas2it.hospitalmanagement.patient.model.Patient" column="PATIENT" cascade="all" not-null="true" /> -->
    <property name="patient" column="PATIENT" type="int"/>
    <property name="physician" column="PHYSICIAN" type="int"/> 
    <property name="patientType" column="PATIENT_TYPE" type="string"/>
    </class>
</hibernate-mapping>